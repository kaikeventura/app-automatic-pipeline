{
  "family": "app-automatic-pipeline-dev-api",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "runtimePlatform": {
    "cpuArchitecture": "ARM64",
    "operatingSystemFamily": "LINUX"
  },
  "executionRoleArn": "arn:aws:iam::729591273848:role/app-automatic-pipeline-dev-ecs-exec-role",
  "taskRoleArn": "arn:aws:iam::729591273848:role/app-automatic-pipeline-dev-ecs-task-role",
  "containerDefinitions": [
    {
      "name": "api",
      "image": "REPLACE_IMAGE",
      "essential": true,
      "portMappings": [{ "containerPort": 8080, "protocol": "tcp" }],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/app-automatic-pipeline-dev-api",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "api"
        }
      }
    }
  ]
}
